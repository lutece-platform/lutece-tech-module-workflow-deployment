<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xmlns:tx="http://www.springframework.org/schema/tx" xmlns:jdbc="http://www.springframework.org/schema/jdbc"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd
       http://www.springframework.org/schema/tx
       http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">
	<!-- Task  -->
	<!-- Task checkout -->
 	<bean id="workflow-deployment.taskTypeCheckout" class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType" 
 		p:key="taskCheckout"
 		p:titleI18nKey="module.workflow.deployment.task_checkout_title"
 		p:beanName="workflow-deployment.taskCheckout"
 		p:taskForAutomaticAction="false" />
 	<bean id="workflow-deployment.taskCheckout" class="fr.paris.lutece.plugins.workflow.modules.deployment.service.TaskCheckout" />
  
  
   <bean id="workflow-deployment.taskTypeAssemblySite" class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType" 
 		p:key="taskAssemblySite"
 		p:titleI18nKey="module.workflow.deployment.task_assembly_site_title"
 		p:beanName="workflow-deployment.taskAssemblySite"
 		p:taskForAutomaticAction="false" />
 	<bean id="workflow-deployment.taskAssemblySite" class="fr.paris.lutece.plugins.workflow.modules.deployment.service.TaskAssemblySite"  />
   
    <bean id="workflow-deployment.taskTypeTag" class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType" 
 		p:key="taskTag"
 		p:titleI18nKey="module.workflow.deployment.task_tag_title"
 		p:beanName="workflow-deployment.taskTag"
 		p:taskForAutomaticAction="false"
 		p:formTaskRequired="true"
  	/>
 	<bean id="workflow-deployment.taskTag" class="fr.paris.lutece.plugins.workflow.modules.deployment.service.TaskTag"  />
 	<bean id="workflow-deployment.taskTypeDeploySite" class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType" 
 		p:key="taskDeploySite"
 		p:titleI18nKey="module.workflow.deployment.task_deploy_site_title"
 		p:beanName="workflow-deployment.taskDeploySite"
 		p:taskForAutomaticAction="false" 
 	/>
 	<bean id="workflow-deployment.taskDeploySite" class="fr.paris.lutece.plugins.workflow.modules.deployment.service.TaskDeploySite" />
 	
 	
 	
 	
 	<!-- Task components -->
	<bean id="workflow-deployment.taskTagComponent" class="fr.paris.lutece.plugins.workflow.modules.deployment.service.TaskTagComponent"
		p:taskType-ref="workflow-deployment.taskTypeTag"/>
 
 </beans>
